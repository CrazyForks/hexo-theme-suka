<%
    suka_std_icon = 'https://theme-suka.github.io/demo/img/suka-favicon.png';
    if (theme.head.favicon.large) {
        suka_std_icon = suka_full_url_for(theme.head.favicon.large);
    } else if (theme.head.favicon.apple_touch_icon) {
        suka_std_icon = suka_full_url_for(theme.head.favicon.apple_touch_icon);
    } else if (theme.head.favicon.medium) {
        suka_std_icon = suka_full_url_for(theme.head.favicon.medium);
    } else if (theme.head.favicon.small) {
        suka_std_icon = suka_full_url_for(theme.head.favicon.small)
    } else if (theme.head.favicon.ico) {
        suka_std_icon = suka_full_url_for(theme.head.favicon.ico);
    }
%>
