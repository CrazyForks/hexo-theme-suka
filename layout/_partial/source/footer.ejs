<!-- vanilla-lazyload -->
<% if (theme.vendors.lazyload) { %>
    <script src="<%= theme.vendors.lazyload %>"></script>
<% } else { %>
    <script src="<%- url_for('lib/vanilla-lazyload/lazyload.min.js') %>"></script>
<% } %>
<script>
var copyrightNow = new Date().getFullYear();
var copyrightContent = document.querySelector('span[year]')
<% if (theme.footer.copyright_since > 0) { %>
    copyrightSince = <%= theme.footer.copyright_since %>;
    if (copyrightSince === copyrightNow) {
        copyrightContent.textContent = copyrightNow
    } else {
        copyrightContent.textContent = copyrightSince + ' - ' + copyrightNow
    }
<% } else { %>
    copyrightContent.textContent = copyrightNow
<% } %>
</script>